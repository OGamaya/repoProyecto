<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Tour" nsURI="http://www.uniandes.edu.co/tour" nsPrefix="Tour">
  <eClassifiers xsi:type="ecore:EClass" name="Tour">
    <eStructuralFeatures xsi:type="ecore:EReference" name="panorama" upperBound="-1"
        eType="#//Panorama" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nombre" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Hotspot">
    <eStructuralFeatures xsi:type="ecore:EReference" name="posicion" lowerBound="1"
        eType="#//Posicion" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rotacion" lowerBound="1"
        eType="#//RotHotspot" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nombre" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dirige" lowerBound="1"
        eType="#//Panorama"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Propiedad">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nombre" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="valor" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Panorama">
    <eStructuralFeatures xsi:type="ecore:EReference" name="roptacionPanorama" eType="#//RotPanorama"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nombre" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="rutaImagen" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="esPartida" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="hotspot" lowerBound="1"
        upperBound="-1" eType="#//Hotspot" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Coordenada" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="x" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="y" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="z" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Plano">
    <eStructuralFeatures xsi:type="ecore:EReference" name="coordenada" lowerBound="1"
        upperBound="-1" eType="#//Coordenada" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Posicion" eSuperTypes="#//Coordenada"/>
  <eClassifiers xsi:type="ecore:EClass" name="RotHotspot" eSuperTypes="#//Coordenada"/>
  <eClassifiers xsi:type="ecore:EClass" name="RotPanorama" eSuperTypes="#//Coordenada"/>
</ecore:EPackage>
